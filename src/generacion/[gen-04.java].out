.CODE
PUSH simple_heap_init
CALL
PUSH lblMetmain@Init
CALL
HALT
simple_heap_init: RET 0	; Retorna inmediatamente
simple_malloc: LOADFP	; Inicialización unidad
LOADSP
STOREFP ; Finaliza inicialización del RA
LOADHL	; hl
DUP	; hl
PUSH 1	; 1
ADD	; hl+1
STORE 4 ; Guarda el resultado (un puntero a la primer celda de la región de memoria)
LOAD 3	; Carga la cantidad de celdas a alojar (parámetro que debe ser positivo)
ADD
STOREHL ; Mueve el heap limit (hl). Expande el heap
STOREFP
RET 1	; Retorna eliminando el parámetro



.DATA
lblVTA: DW lblMetm1@A,lblMetsetall@A

.CODE

lblConstructor@A: LOADFP  ; Apila el valor del registro fp
LOADSP  ; Apila el valor del registro sp
STOREFP  ; Almacena el tope de la pila en el registro fp
FMEM 0
STOREFP  ; Almacena el tope de la pila en el registro fp
RET 1

lblMetm1@A: LOADFP  ; Apila el valor del registro fp
LOADSP  ; Apila el valor del registro sp
STOREFP  ; Almacena el tope de la pila en el registro fp
LOAD 3  ; Cargo this 
LOADREF 1
PUSH lblMetdebugPrint@Object  ; Apila el método 
CALL  ; Llama al método en el tope de la pila
LOAD 3  ; Cargo this 
LOADREF 2
PUSH lblMetdebugPrint@Object  ; Apila el método 
CALL  ; Llama al método en el tope de la pila
LOAD 3  ; Cargo this 
LOADREF 3
PUSH lblMetprintSln@System  ; Apila el método 
CALL  ; Llama al método en el tope de la pila
FMEM 0
STOREFP  ; Almacena el tope de la pila en el registro fp
RET 1

lblMetsetall@A: LOADFP  ; Apila el valor del registro fp
LOADSP  ; Apila el valor del registro sp
STOREFP  ; Almacena el tope de la pila en el registro fp
LOAD 4 ; Apilo el valor en memoria del offset de p1
LOAD 3  ; Cargo this 
SWAP
STOREREF 1
LOAD 3  ; Cargo this 
LOADREF 1
PUSH 2  ; Apila el entero 2
MUL
LOAD 3  ; Cargo this 
SWAP
STOREREF 2
.DATA
string1: DW "hola",0  ; Apilo el string
.CODE
PUSH string1
LOAD 3  ; Cargo this 
SWAP
STOREREF 3
FMEM 0
STOREFP  ; Almacena el tope de la pila en el registro fp
RET 2

